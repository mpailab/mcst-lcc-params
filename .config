# Пример конфигурационного файла

# Каталог с файлами весов процедур спеков
dir_w_proc = "./spec/stat/exec"

# Файл с весами спеков
file_w_task = "./spec/stat/exec/spec_weights.txt"

# Каталог со статистикой компиляции спеков
stat = "./spec/stat/comp_all/stat"

# Каталог, в который собирается статистика компиляции спеков во время работы ИС
din_stat = "./spec/stat/comp_all/stat"

# Путь до скрипта, который запускает компиляцию спека, и возвращает время компиляции
SCRIPT_COMP = "./choice/__run_comp.sh"

# Путь до скрипта, который запускает исполнение спека, и возвращает время исполнения
SCRIPT_EXEC = "./choice/__run_exec.sh"

# Путь до скрипта, который запускает компиляцию спека вместе со сбором статистики, и возвращает объем потребляемой памяти
SCRIPT_COMP_WITH_STAT = "./choice/__run_comp_with_stat.sh"

# Стратегия оптимизации
strategy = "regn_max_proc_op_sem_size; regn_opers_limit; regn_heur1 regn_heur2 regn_heur3 regn_heur4; disable_regions_nesting; ifconv_opers_num ifconv_calls_num ifconv_merge_heur; dcs"

# Список оптимизируемых спеков
specs = "502.gcc, 500.perlbench, 503.bwaves, 505.mcf, 508.namd, 510.parest, 511.povray, 519.lbm, 521.wrf, 523.xalancbmk, 525.x264, 526.blender, 527.cam4, 531.deepsjeng, 538.imagick, 541.leela, 544.nab, 548.exchange2, 549.fotonik3d, 554.roms, 557.xz"

# Каталог, в котором формируются выходные файлы
output = "./tmp"
